{
  "$schema": "http://json-schema.org/schema",
  "$id": "SchematicsMbcEntity",
  "title": "Mbc-cqrs-serverless Entity Options Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the entity.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the entity?"
    }
  },
  "required": ["name"]
}
