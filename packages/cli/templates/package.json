  "scripts": {
    "postinstall": "prisma generate",
    "build": "nest build --watch",
    "build:prod": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:repl": "nest start --watch --entryFile repl",
    "offline:docker:build": "cd infra-local && docker-compose up --build --remove-orphans",
    "offline:docker": "cd infra-local && docker-compose up --remove-orphans",
    "offline:sls": "ln -f .env $PWD/infra-local/.env && cd infra-local && cp -r cognito-local/db docker-data/.cognito && NODE_ENV=development AWS_ACCESS_KEY_ID=DUMMYIDEXAMPLE AWS_SECRET_ACCESS_KEY=DUMMYEXAMPLEKEY SLS_DEBUG=* serverless offline start --reloadHandler --ignoreJWTSignature",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:dev": "prisma migrate dev",
    "migrate:rds": "prisma migrate deploy",
    "migrate:ddb": "ts-node prisma/ddb.ts",
    "migrate": "npm run migrate:rds && npm run migrate:ddb"
  },
